{
    "branches": [
        {
            "name": "%PROD_BRANCH%",
            "prerelease": %PROD_IS_PRERELEASE%
        },
        {
            "name": "%TEST_BRANCH%",
            "prerelease": %TEST_IS_PRERELEASE%
        },
        {
            "name": "%DEV_BRANCH%",
            "prerelease": %DEV_IS_PRERELEASE%
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "bin/**",
                    }
                ]
            }
        ]
    ]
}
